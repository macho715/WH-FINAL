    "CNTR Stuffing Q'TY": "hasQuantity",
    "pkgs": "hasPackageCount",
    "Weight (kg)": "hasWeight",
    "CBM": "hasVolume",
    "TOTAL": "hasAmount",      
    "Amount": "hasAmount"      
  },
  "datatype_properties": [
    "hasDate",
    "hasQuantity",
    "hasPackageCount",
    "hasWeight",
    "hasVolume",
    "hasAmount"                  
  ],
  "class_map": {
    "Outdoor": "OutdoorWarehouse",
    "Indoor(M44)": "IndoorWarehouseM44"
  }
